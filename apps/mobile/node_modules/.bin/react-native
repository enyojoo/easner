#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/react-native@0.74.3_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__@types+react@18.0.0_react@18.2.0/node_modules/react-native/node_modules:/workspace/node_modules/.pnpm/react-native@0.74.3_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__@types+react@18.0.0_react@18.2.0/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/react-native@0.74.3_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__@types+react@18.0.0_react@18.2.0/node_modules/react-native/node_modules:/workspace/node_modules/.pnpm/react-native@0.74.3_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__@types+react@18.0.0_react@18.2.0/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
