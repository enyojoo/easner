#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules/expo/bin/node_modules:/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules/expo/node_modules:/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules/expo/bin/node_modules:/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules/expo/node_modules:/workspace/node_modules/.pnpm/expo@51.0.39_@babel+core@7.28.4_@babel+preset-env@7.28.3_@babel+core@7.28.4__react-nati_fae4800b0f3b149a2f7f233b57c8391b/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/cli" "$@"
else
  exec node  "$basedir/../expo/bin/cli" "$@"
fi
