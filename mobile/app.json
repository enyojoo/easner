{
  "expo": {
    "name": "Easner",
    "slug": "easner",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easner.mobile",
      "icon": "./assets/icons/ios-light.png",
      "darkIcon": "./assets/icons/ios-dark.png",
      "tintedIcon": "./assets/icons/ios-tinted.png",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/icons/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#007ACC",
        "monochromeImage": "./assets/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.easner.mobile",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/icons/ios-light.png"
    },
    "extra": {
      "eas": {
        "projectId": "f50954cf-ded8-4635-a858-23a1e11aa758"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-image-picker",
      "expo-document-picker",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#007ACC",
          "image": "./assets/icons/splash-icon-light.png",
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/icons/splash-icon-dark.png",
            "backgroundColor": "#007ACC"
          },
          "imageWidth": 200
        }
      ]
    ],
    "owner": "enyosam"
  }
}
