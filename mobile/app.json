{
  "expo": {
    "name": "Easner",
    "slug": "easner",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.easner.mobile",
      "icon": "./assets/icons/ios-light.png",
      "darkIcon": "./assets/icons/ios-dark.png",
      "tintedIcon": "./assets/icons/ios-tinted.png",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:easner.com"]
    },
    "android": {
      "icon": "./assets/icons/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#007ACC",
        "monochromeImage": "./assets/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.easner.mobile",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "easner.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icons/ios-light.png"
    },
    "extra": {
      "eas": {
        "projectId": "b1e3438a-faea-4359-a86f-6b279ebe2743"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-image-picker",
      "expo-document-picker",
      [
        "expo-splash-screen",
        {
          "image": "./assets/onboarding/Splash.png",
          "resizeMode": "cover",
          "dark": {
            "image": "./assets/onboarding/Splash.png"
          }
        }
      ]
    ],
    "owner": "easner"
  }
}
